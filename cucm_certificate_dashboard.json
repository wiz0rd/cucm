{
  "dashboard": {
    "id": null,
    "title": "CUCM Certificate Monitoring Dashboard",
    "tags": ["cucm", "certificates", "ssl", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5m",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5m", "15m", "30m", "1h", "2h", "1d"]
